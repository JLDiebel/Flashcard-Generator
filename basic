var askQuestion = function() {
 
  if (count < 5) {
    inquirer.prompt([
      {
        type: "input",
        message: array[0].front,
        name: "answer"
      }

    ]).then(function(answers) {
      if (answers = question[count].back){
        console.log("You're correct! Radical!")
